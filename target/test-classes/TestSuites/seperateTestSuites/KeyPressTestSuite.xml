<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="KeyPressTestSuite" verbose="1">

	<test name="KeyPressesTest-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.herokuapp.theinternet.keypressTests.KeyPressTest">
				<methods>
					<include name="pressKeyWithActions"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="KeyPressesTest-FF">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.herokuapp.theinternet.keypressTests.KeyPressTest">
<!--				<methods>-->
<!--					<exclude name="pressKeyWithActions"></exclude>-->
<!--				</methods>-->
			</class>
		</classes>
	</test>

</suite>